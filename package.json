{
   "name": "cog-sync",
   "version": "1.0.0",
   "private": false,
   "description": "Settings sync cli",
   "repository": "https://github.com/yonycalsin/cog-sync.git",
   "license": "MIT",
   "author": "Yony Calsin",
   "main": "dist/main.js",
   "scripts": {
      "build": "run-s clean build:*",
      "build:tsc": "tsc",
      "clean": "rimraf dist",
      "dev": "tsnd --respawn -T -r tsconfig-paths/register src/main.ts",
      "format": "prettier --write ./**/*.{ts,json,js}",
      "start": "node dist/main.js"
   },
   "devDependencies": {
      "npm-run-all": "^4.1.5",
      "prettier": "^2.2.0",
      "prettier-plugin-organize-imports": "^1.1.1",
      "prettier-plugin-packagejson": "^2.2.8",
      "ts-node-dev": "^1.0.0-pre.63",
      "tsconfig-paths": "^3.9.0",
      "typescript": "^4.0.3"
   }
}
